{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "davy39/composer-codeflow",
    "type": "project",
    "description": "Install composer proxy plugin locally .",
    "require": {
        "custom/composer-proxy": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "custom/composer-proxy": true
        }
    },
    "repositories": [
        {
            "packagist.org": false
        },
        {
            "type": "path",
            "url": "./composer-proxy",
            "options": {
                "symlink": true
            }
        }
    ]
}
